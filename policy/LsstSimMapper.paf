#<?cfg paf policy ?>

camera:        "../description/Full_STA_geom.paf"
defects:    "../description/defects"

needCalibRegistry: false

levels: {
    skyTile: "visit" "raft" "sensor"
    visit: "snap" "raft" "sensor" "channel"
    raft: "snap" "sensor" "channel"
    sensor: "snap" "channel"
    snap: "channel"
    tract: "patch"
}
defaultLevel: "sensor"
defaultSubLevels: {
    skyTile: "sensor"
    visit: "sensor"
    raft: "sensor"
    sensor: "channel"
}

exposures: {
