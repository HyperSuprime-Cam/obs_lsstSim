#<?cfg paf dictionary ?>

target: LsstSimMapper

definitions: {
    rawTemplate: {
        type: string
        description: "The Python substitution template for the raw image pathnames."
        maxOccurs: 1
	default: "raw/v%(visit)d-f%(filter)s/E%(snap)03d/R%(raft)s/S%(sensor)s/imsim_%(visit)d_R%(raft)s_S%(sensor)s_C%(channel)s_E%(snap)03d.fits.gz"
    }
    eimageTemplate: {
        type: string
        description: "The Python substitution template for the electron image pathnames."
        maxOccurs: 1
	default: "eimage/v%(visit)d-f%(filter)s/E%(snap)03d/R%(raft)s/eimage_%(visit)d_R%(raft)s_S%(sensor)s_E%(snap)03d.fits.gz"
    }
    biasTemplate: {
        type: string
        description: "The Python substitution template for the bias image pathnames."
        maxOccurs: 1
	default: "bias/v0/R%(raft)s/S%(sensor)s/imsim_0_R%(raft)s_S%(sensor)s_C%(channel)s.fits"
    }
    darkTemplate: {
        type: string
        description: "The Python substitution template for the dark image pathnames."
        maxOccurs: 1
	default: "dark/v1/R%(raft)s/S%(sensor)s/imsim_1_R%(raft)s_S%(sensor)s_C%(channel)s.fits"
    }
    flatTemplate: {
        type: string
        description: "The Python substitution template for the flat image pathnames."
        maxOccurs: 1
	default: "flat/v2-f%(filter)s/R%(raft)s/S%(sensor)s/imsim_2_R%(raft)s_S%(sensor)s_C%(channel)s.fits"
    }
    fringeTemplate: {
        type: string
        description: "The Python substitution template for the fringe image pathnames."
        maxOccurs: 1
	default: "flat/v2-f%(filter)s/R%(raft)s/S%(sensor)s/imsim_2_R%(raft)s_S%(sensor)s_C%(channel)s.fits"
    }
    postISRTemplate: {
        type: string
        description: "The Python substitution template for the post-ISR amplifier image pathnames."
        maxOccurs: 1
	default: "postISR/v%(visit)d-f%(filter)s/s%(snap)d/R%(raft)s/S%(sensor)s/C%(channel)s.fits"
    }
    postISRCCDTemplate: {
        type: string
        description: "The Python substitution template for the post-ISR CCD image pathnames."
        maxOccurs: 1
	default: "postISRCCD/v%(visit)d-f%(filter)s/s%(snap)d/R%(raft)s/S%(sensor)s.fits"
    }
    sdqaAmpTemplate: {
        type: string
        description: "The Python substitution template for the amp-level SDQA ratings pathnames."
        maxOccurs: 1
        default: "sdqaAmp/v%(visit)d-f%(filter)s/s%(snap)d/R%(raft)s/S%(sensor)s/C%(channel)s.boost"
    }
    sdqaCcdTemplate: {
        type: string
        description: "The Python substitution template for the CCD-level SDQA ratings pathnames."
        maxOccurs: 1
        default: "sdqaCcd/v%(visit)d-f%(filter)s/s%(snap)d/R%(raft)s/S%(sensor)s.boost"
    }
    visitimTemplate: {
        type: string
        description: "The Python substitution template for the CR-removed visit image pathnames."
        maxOccurs: 1
	default: "visitim/v%(visit)d-f%(filter)s/R%(raft)s/S%(sensor)s.fits"
    }
    icSrcTemplate: {
        type: string
        description: "The Python substitution template for the image characterization source pathnames."
        maxOccurs: 1
	default: "icSrc/v%(visit)d-f%(filter)s/R%(raft)s/S%(sensor)s.boost"
    }
    psfTemplate: {
        type: string
        description: "The Python substitution template for the measured PSF pathnames."
        maxOccurs: 1
	default: "psf/v%(visit)d-f%(filter)s/R%(raft)s/S%(sensor)s.boost"
    }
    calexpTemplate: {
        type: string
        description: "The Python substitution template for the calibrated science exposure pathnames."
        maxOccurs: 1
	default: "calexp/v%(visit)d-f%(filter)s/R%(raft)s/S%(sensor)s.fits"
    }
    srcTemplate: {
        type: string
        description: "The Python substitution template for the source vector pathnames."
        maxOccurs: 1
	default: "src/v%(visit)d-f%(filter)s/R%(raft)s/S%(sensor)s.boost"
    }
    sourceHistTemplate: {
        type: string
        description: "The Python substitution template for the source histogram pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/sourceHist.fits"
    }
    badSourceHistTemplate: {
        type: string
        description: "The Python substitution template for the bad source histogram pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/badSourceHist.fits"
    }
    sourceTemplate: {
        type: string
        description: "The Python substitution template for the final source vector pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/source.boost"
    }
    badSourceTemplate: {
        type: string
        description: "The Python substitution template for the bad source vector pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/badSource.boost"
    }
    invalidSourceTemplate: {
        type: string
        description: "The Python substitution template for the invalid source vector pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/invalidSource.boost"
    }
    objectTemplate: {
        type: string
        description: "The Python substitution template for the source cluster pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/object.boost"
    }
    badObjectTemplate: {
        type: string
        description: "The Python substitution template for the bad source cluster pathnames."
        maxOccurs: 1
	default: "results/st%(skyTile)d/badObject.boost"
    }


    root: {
        type: string
        description: "The pathname to the root directory for images."
        maxOccurs: 1
    }
    calibRoot: {
        type: string
        description: "The pathname to the calibration root directory."
        maxOccurs: 1
    }
    registryPath: {
        type: string
        description: "The pathname to the image registry.  Can be absolute, relative to the root, or relative to the current directory."
        maxOccurs: 1
    }
    skypixPolicy: {
        type: policy
        # description: "Subpolicy for configuring the sky pixelization."
        maxOccurs: 1
    }
    cameraDescription: {
        type: string
        description: "The pathname to the camera description (in PAF form).  Can be absolute or relative to the repository containing this dictionary."
        maxOccurs: 1
        default: "../description/Full_STA_geom.paf"
    }
    defectPath: {
        type: string
        description: "The pathname to the directory containing the defect policies and registry.  Relative to the repository containing this dictionary."
        maxOccurs: 1
        default: "../description/defects"
    }
    filterDescription: {
        type: string
        description: "The pathname to the filter description (in PAF form).  Can be absolute or relative to the repository containing this dictionary."
        default: "../description/LsstFilters.paf"
    }
}
